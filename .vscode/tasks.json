{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "Build VST3 (debug)",
      "command": "cmake",
      "options": {
        "cwd": "${workspaceRoot}"
      },
      "args": [
        "--build",
        "./build",
        "--config",
        "Debug",
        "--target",
        "3DGuitarAmpRenderer_VST3",
        "--",
        "-j",
        "10"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "type": "shell",
      "label": "Build AU (debug)",
      "command": "cmake",
      "options": {
        "cwd": "${workspaceRoot}"
      },
      "args": [
        "--build",
        "./build",
        "--config",
        "Debug",
        "--target",
        "3DGuitarAmpRenderer_AU",
        "--",
        "-j",
        "10"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
